<ViewObject
   Name="RIMHeaderSummVO"
   OrderBy="transaction_no"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="xxup.oracle.apps.per.rim.server.RIMHeaderSummVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT *
FROM (
SELECT transaction_no,
    assignment_id,
    research_type,
    research_title,
    research_title2,
    research_title3,
    brief_description,
    project_impact_desc,
    project_leader_id,
    start_date,
    actual_end_date,
    project_status,
    project_remarks,
    item_key,
    approval_status,
    attribute1,
    attribute2,
    attribute3,
    attribute4,
    attribute5,
    last_update_date,
    last_updated_by,
    last_update_login,
    created_by,
    creation_date,
    research_type_spec
    ,DECODE(hd.approval_status, 'Approved', 'ENABLED'
                              , 'Saved For Later', 'SFL'
            ,'DISABLED') SUMM_PG_AC
    ,ROW_NUMBER() OVER(PARtitION BY transaction_no ORDER BY CASE WHEN approval_status = 'Approved' OR approval_status = 'Closed'
                                                                 THEN 2
                                                                 WHEN approval_status = 'For Approval'
                                                                 THEN 1
                                                                 ELSE 3
                                                            END
                                                            ) appr_stat_rn
    ,LPAD(hd.TRANSACTION_NO,9,'0') transaction_no_disp
FROM xxup_rim_header hd
)
WHERE appr_stat_rn = 1
   ]]></SQLQuery>
